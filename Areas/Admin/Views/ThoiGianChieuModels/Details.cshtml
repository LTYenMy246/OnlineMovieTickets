@model OnlineMovieTicket.Models.ThoiGianChieuModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}
<div class="col-12 grid-margin">
    <div class="card">
        <div class="card-body">
            <h3>Chi tiết thời gian chiếu</h3>
            <div>
                <hr />
                <dl class="row">
                    <dt class="col-sm-2">
                        <lable>Thời gian chiếu:</lable>
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.ThoiGianChieu)
                    </dd>
                </dl>
            </div>
            <div>
                <a asp-action="Edit" asp-route-id="@Model?.MaThoiGianChieu" class="btn btn-primary">Chỉnh sửa</a> |
                <a asp-action="Index" class="btn btn-primary">Trở về danh sách thời gian chiếu</a>
            </div>
        </div>
    </div>
</div>

